<?xml version="1.0" encoding="UTF-8"?><xmi:XMI xmlns:xmi="http://www.omg.org/XMI" xmlns:metadata="http:///edu/ucdenver/ccp/nlp/core/uima/annotation/metadata.ecore" xmlns:mention="http:///edu/ucdenver/ccp/nlp/core/uima/mention.ecore" xmlns:tcas="http:///uima/tcas.ecore" xmlns:cas="http:///uima/cas.ecore" xmlns:assertion="http:///edu/ucdenver/ccp/nlp/core/uima/assertion.ecore" xmlns:annotation="http:///edu/ucdenver/ccp/nlp/core/uima/annotation.ecore" xmi:version="2.0"><cas:NULL xmi:id="0"/><tcas:DocumentAnnotation xmi:id="2" sofa="1" begin="0" end="40199" language="en"/><annotation:CCPDocumentInformation xmi:id="3" documentID="16433929" documentCollectionID="0" documentSize="0" encoding="UTF_8"/><annotation:CCPTextAnnotation xmi:id="4" sofa="1" begin="67" end="71" annotationID="0" documentSectionID="0" numberOfSpans="1" spans="6" owlClass="NCBITaxon:10088"/><annotation:CCPTextAnnotation xmi:id="7" sofa="1" begin="445" end="450" annotationID="0" documentSectionID="0" numberOfSpans="1" spans="9" owlClass="NCBITaxon:10088"/><annotation:CCPTextAnnotation xmi:id="10" sofa="1" begin="623" end="632" annotationID="0" documentSectionID="0" numberOfSpans="1" spans="12" owlClass="NCBITaxon:40674"/><annotation:CCPTextAnnotation xmi:id="13" sofa="1" begin="702" end="706" annotationID="0" documentSectionID="0" numberOfSpans="1" spans="15" owlClass="NCBITaxon:10088"/><annotation:CCPTextAnnotation xmi:id="16" sofa="1" begin="850" end="854" annotationID="0" documentSectionID="0" numberOfSpans="1" spans="18" owlClass="NCBITaxon:10088"/><annotation:CCPTextAnnotation xmi:id="19" sofa="1" begin="968" end="973" annotationID="0" documentSectionID="0" numberOfSpans="1" spans="21" owlClass="NCBITaxon:10088"/><annotation:CCPTextAnnotation xmi:id="22" sofa="1" begin="1057" end="1062" annotationID="0" documentSectionID="0" numberOfSpans="1" spans="24" owlClass="NCBITaxon:10088"/><annotation:CCPTextAnnotation xmi:id="25" sofa="1" begin="1098" end="1105" annotationID="0" documentSectionID="0" numb